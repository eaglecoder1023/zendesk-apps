    <form action="http://skipjack.info/?s={{searchCasper}}" method="post" target="_blank"> 
         <input style="cursor: pointer;" name="search" value="Search skipjack: {{searchCasper}}" type="submit" size="30"/> 
    </form>
    <form name="ir">
