    <option value="https://www.google.com/search?q={{requestEmail}}">Lookup this {{requestEmail}} Gifts</option>
    </select>
    <input type="button" onClick="window.open(document.ir.menu.options[document.ir.menu.selectedIndex].value);" value="Go">
    </form>
    <p>
    <p>&nbsp;</p>

  </div>
</section>